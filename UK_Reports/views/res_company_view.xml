<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_company_form_uk_reports">
            <field name="name">res.company.form.uk_reports</field>
            <field name="model">res.company</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <field name="website" position="after">
					<field name="accounts_dept_phone"/>
					<field name="accounts_dept_email"/>
					<field name="purchase_dept_phone"/>
					<field name="purchase_dept_email"/>
					<field name="sales_dept_phone"/>
					<field name="sales_dept_email"/>
                </field>
            </field>
        </record>    

    </data>
</openerp>
